{
  "name": "portal",
  "version": "1.1.0",
  "description": "OneX admin portal based on ElementUI",
  "author": "Charles <zhangchaoxu@gmail.com>",
  "license": "Apache License Version 2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:test": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build --mode production",
    "build:uat": "vue-cli-service build --mode production.uat",
    "lint": "vue-cli-service lint",
    "et": "node_modules/.bin/et",
    "et:init": "node_modules/.bin/et -i",
    "et:list": "gulp themes"
  },
  "dependencies": {
    "ali-oss": "^6.12.0",
    "axios": "^0.21.0",
    "babel-plugin-component": "^1.1.1",
    "crypto-js": "^4.0.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.14.1",
    "element-ui": "^2.14.1",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-load-plugins": "^2.0.3",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "dayjs": "^1.9.7",
    "node-sass": "^4.14.1",
    "qs": "^6.9.4",
    "quill": "^1.3.7",
    "sass-loader": "^10.0.2",
    "screenfull": "^5.0.2",
    "svg-sprite-loader": "^5.0.0",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.2",
    "vue-json-viewer": "^2.2.16",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "@antv/g2plot": "^2.3.2",
    "@chenfengyuan/vue-qrcode": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/eslint-config-standard": "^3.0.5",
    "vue-template-compiler": "^2.6.12",
    "babel-eslint": "8.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 8.11.1",
    "npm": ">= 5.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "element-theme": {
    "config": "./src/element-ui/theme-variables.scss",
    "out": "./src/element-ui/theme",
    "minimize": true,
    "browsers": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  }
}
